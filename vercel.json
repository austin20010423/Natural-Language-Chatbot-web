{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        }
        {
            "src": "vercel.json", 
            "use": "vercel-sapper"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
