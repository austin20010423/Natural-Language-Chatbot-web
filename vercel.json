{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        },
        {
             "src": "api/templates/form.html",
             "use": "@vercel/html"
        },
        {
              "src": "api/static/style/style.css",
              "use": "@vercel/css"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
