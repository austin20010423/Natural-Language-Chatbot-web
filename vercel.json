{
    "version": 2,
    "builds": [
        {
            "src": "Templates/.html",
            "use": "@vercel/Templates"
        },
        {
            "src": "*.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/.css",
            "use": "@vercel/static/style"
        },
        {
            "src": "static/.jpg",
            "use": "@vercel/static/pic"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/form.html"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404.html"
        },
        {
            "src": "/",
            "dest": "/app.py"
        }
    ]
}